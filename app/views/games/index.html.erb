<h1>Links Made</h1>

<table class='results'>
  <tr>
    <td>Starting Actor</td>
    <td>Ending Actor</td>
    <td>Steps</td>
  </tr>
<% @games.each do |game| %>
  <tr>
    <td><%= render partial: 'actors/profile', locals: {actor: game.actor_start} %></td>
    <td><%= render partial: 'actors/profile', locals: {actor: game.actor_end} %></td>
    <td><%= game.steps %></td>
  </tr>
<% end %>
</table>